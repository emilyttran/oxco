<% include partials/header %>

<h1>Home</h1>
<% projects.forEach(function(project){ %>
    <div class="col-md-3 col-sm-6">
        <a href="#">
        <div class="thumbnail">
            <img src="<%= project.img %>">
            <div class="caption">
                <h3><%= project.title %></h3>
                <h1><%= project.name %></h1>
            </div>
        </div>
        </a>
    </div>
<% }); %>

<% include partials/footer %>